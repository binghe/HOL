all: $(DEFAULT_TARGETS)
.PHONY: all

# Disable selftests (designed for cv_compute) in otknl mode
ifneq ($(KERNELID),otknl)
all: selftest.exe

selftest.exe: selftest.uo reduceLib.uo
	$(HOLMOSMLC) -o $@ $<

EXTRA_CLEANS = selftest.exe reduce-selftest.log

ifdef HOLSELFTESTLEVEL
all: reduce-selftest.log

reduce-selftest.log: selftest.exe
	$(tee ./$<,$@)
endif
endif
